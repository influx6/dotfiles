<root xmlns:xlink="http://www.w3.org/1999/xlink">
  <g id="acyl-filter">
    <filter id="filter">
      <feTurbulence id="feTurbulence1" type="turbulence" result="result0" seed="5" numOctaves="3.0" baseFrequency="0 0.20"/>
      <feTurbulence id="feTurbulence2" type="turbulence" seed="5" numOctaves="3.0" baseFrequency="0.30 0"/>
      <feComposite id="feComposite1" in2="result0" result="result1" operator="over"/>
      <feMerge id="feMerge1" result="result2">
        <feMergeNode id="feMergeNode1" in="result1"/>
        <feMergeNode id="feMergeNode2" in="result0"/>
      </feMerge>
      <feComposite id="feComposite2" in2="result2" in="SourceGraphic" operator="in"/>
    </filter>
  </g>
  <g id="acyl-visual">
    <use id="visible1" transform="translate(24,24) scale(1.00) translate(-24,-24)" style="fill:url(#acyl-gradient);filter:url(#filter)" xlink:href="#acyl-drawing"/>
  </g>
</root>
